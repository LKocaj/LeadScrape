{
  "name": "leadscrape",
  "version": "1.0.0",
  "description": "Enterprise lead scraper for OnCall Automation",
  "type": "module",
  "main": "dist/index.js",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "bin": {
    "leadscrape": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:web": "npm run build -w packages/web",
    "build:api": "npm run build -w packages/api",
    "build:all": "npm run build && npm run build:api && npm run build:web",
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "dev:api": "npm run dev -w packages/api",
    "dev:web": "npm run dev -w packages/web",
    "start": "node dist/index.js",
    "start:api": "npm run start -w packages/api",
    "cli": "tsx src/cli.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "lead-generation",
    "scraper",
    "automation"
  ],
  "author": "OnCall Automation",
  "license": "UNLICENSED",
  "dependencies": {
    "axios": "^1.7.9",
    "bullmq": "^5.34.8",
    "cheerio": "^1.0.0",
    "commander": "^13.0.0",
    "dotenv": "^16.4.7",
    "exceljs": "^4.4.0",
    "fuse.js": "^7.0.0",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.4.2",
    "libphonenumber-js": "^1.11.18",
    "nanoid": "^5.0.9",
    "opossum": "^8.4.2",
    "p-queue": "^8.0.1",
    "pino": "^9.6.0",
    "playwright": "^1.49.1",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "socks-proxy-agent": "^8.0.5",
    "sql.js": "^1.12.0",
    "string-similarity": "^4.0.4",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "concurrently": "^9.1.2",
    "@types/opossum": "^8.1.7",
    "@types/sql.js": "^1.4.9",
    "@types/string-similarity": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.18.0",
    "nock": "^14.0.1",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
